{
    "builds": [{
        "src": "bakery_management/wsgi.py",
        "use": "@vercel/python",
        "config": { 
            "maxLambdaSize": "15mb" ,
            "buildCommand": "sh ./build_files.sh"
            }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "bakery_management/wsgi.py"
        }
    ]
}